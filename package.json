{
	"name": "flaiir",
	"displayName": "Flaiir",
	"description": "An AI tool that utilizes the OpenAI API to generate code suggestions, fixes spelling mistakes, refactor existing code and explains code for the user, based on the current text editors selection and file type.",
	"version": "0.0.1",
	"license": "SEE LICENSE IN LICENSE",
	"publisher": "flaiir",
	"icon": "images/flaiir.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/MadsHaerup/flaiir"
	},
	"engines": {
		"vscode": "^1.74.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "flaiir.getCodeSuggestions.start",
				"title": "flaiir generator/explainer"
			},
			{
				"command": "flaiir.getCodeEdit.start",
				"title": "flaiir refactor"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^8.0.0",
		"@types/mocha": "^10.0.1",
		"@types/node": "16.x",
		"@types/vscode": "^1.74.0",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"@vscode/test-electron": "^2.2.0",
		"eslint": "^8.28.0",
		"glob": "^8.0.3",
		"mocha": "^10.1.0"
	},
	"dependencies": {
		"typescript": "^4.9.3",
		"axios": "^1.2.2",
		"openai": "^3.1.0"
	}
}
